<%- include('partials/header') %>


    <!--====form section start====-->
    <div class="container-fluid summary-report-table col-md jumbotron ">
        <h3>Product Number: <%= productNumber %>
        </h3>
        <h5>Date: <%= date %>, Time: <%= time %>
        </h5>
        <h5>Water Filling Time: <%= wft %></h5>
        <h5>Water Heating Time: <%= wht %></h5>
        <div class="table-wrapper-scroll-x my-custom-scrollbar">
            <table class="table table-dark table-hover table-data" cellspacing="0" width="100%">
                <tr>
                    <th>Itr</th>
                    <th>Criteria</th>
                    <th>Gear Box1</th>
                    <th>Gear Box2</th>
                    <th>Gear Box3</th>
                    <th>Mixer1</th>
                    <th>Mixer2</th>
                    <th>Mixer3</th>
                    <th>Water in Mixer1</th>
                    <th>Water in Mixer2</th>
                    <th>Water in Mixer3</th>
                    <th>Temp</th>
                    <th>Weight</th>
                    <th>Status</th>

                </tr>

                <% cycles.forEach(function(data,index) { %>
                    <tr>
                        <td>
                            <%= index+1 %>
                        </td>

                
                                    <td>
                                        <%=data.Criteria%>
                                    </td>
                                    <td>
                                        <%=data.GearBox1%>
                                    </td>
                                    <td>
                                        <%=data.GearBox2%>
                                    </td>
                                    <td>
                                        <%=data.GearBox3%>
                                    </td>
                                    <td>
                                        <%=data.Mixer1%>
                                    </td>
                                    <td>
                                        <%=data.Mixer2%>
                                    </td>
                                    <td>
                                        <%=data.Mixer3%>
                                    </td>
                                    <td>
                                        <%=data.WaterMixer1%>
                                    </td>
                                    <td>
                                        <%=data.WaterMixer2%>
                                    </td>
                                    <td>
                                        <%=data.WaterMixer3%>
                                    </td>
                                    <td>
                                        <%=data.Temp%>
                                    </td>
                                    <td>
                                        <%=data.Weight%>
                                    </td>
                                    <td>
                                        <%=data.Status%>
                                    </td>

                                    <% if(data.Status==='OK' ) { %>
                                        <td>
                                            <p style="color:#A8F72E">PASS</p>
                                        </td>
                                        <% } else { %>
                                            <td>
                                                <p style="color:#FF3C00">Fail</a>
                                            </td>
                                            <% } %>

                    </tr>
                    <% }) %>
            </table>
        </div>
    </div>

    <%- include('partials/footer') %>